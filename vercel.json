{
  "version": 2,
  "buildCommand": "cd dpm-web && npm run build",
  "outputDirectory": "dpm-web/dist",
  "installCommand": "cd dpm-web && npm install",
  "rewrites": [
    { "source": "/api/(.*)", "destination": "/api" },
    { "source": "/(.*)", "destination": "/index.html" }
  ],
  "env": {
    "NODE_ENV": "production",
    "VITE_DEMO_MODE": "true"
  }
  // Updated: 2025-11-18 - Trigger deployment with all TypeScript fixes
}